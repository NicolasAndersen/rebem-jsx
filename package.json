{
  "name": "babel-plugin-transform-rebem-jsx",
  "version": "0.1.0",
  "description": "Babel plugin for reBEM JSX transformations",
  "keywords": [ "rebem", "react", "bem", "jsx" ],
  "homepage": "https://github.com/rebem/rebem-jsx",
  "repository": "rebem/rebem-jsx",
  "contributors": [
    "Kir Belevich <kir@soulshine.in> (https://github.com/deepsweet)",
    "Denis Koltsov <iam@mistadikay.com> (https://github.com/mistadikay)"
  ],
  "main": "build/index.js",
  "files": [ "build/" ],
  "dependencies": {
    "babel-cli": "6.4.x",
    "babel-core": "6.4.x",
    "babel-plugin-transform-class-properties": "6.4.x",
    "babel-plugin-transform-runtime": "6.4.x",
    "babel-preset-es2015": "6.3.x",
    "babel-preset-stage-1": "6.3.x",
    "babel-preset-react": "6.3.x",
    "babel-register": "6.4.x"
  },
  "devDependencies": {
    "chokidar-cli": "1.2.x",
    "chalk": "1.1.x",
    "clear": "0.0.x",
    "diff": "2.2.x",
    "watch": "0.17.x",

    "eslint": "1.10.x",
    "babel-eslint": ">5.0.0-beta1",
    "eslint-plugin-babel": "3.1.x",
    "eslint-config-rebem": "0.3.x",

    "husky": "0.10.x"
  },
  "scripts": {
    "build": "babel lib/ -d build/",

    "lint": "eslint ./src/index.js",
    "tdd": "chokidar ./**/*.js -c 'npm test' --initial",
    "test": "npm run lint",
    "travis": "npm test",

    "precommit": "npm run lint",
    "prepublish": "npm run build"
  },
  "engines": {
    "node": ">=0.12.0",
    "npm": ">=2.7.0"
  },
  "license": "MIT"
}
